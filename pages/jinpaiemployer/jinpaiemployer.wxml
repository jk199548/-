<view class='box'>
<!-- 头部选择地区 -->
  <view class='head'>
    <view class='region'>
      <picker mode='region' value='{{regionarr}}' bindchange='changeregion'>{{province}}</picker>
      <image class='downimage' src='../../images/index/hot-detail-lookmore-icon.png' mode='aspectFit'></image>
    </view>
    <view class='region'>
