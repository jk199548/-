    </view>
    <view class='region'>
      <picker mode='region' value='{{regionarr}}' bindchange='changeregion'>{{area}}</picker>
      <image class='downimage' src='../../images/index/hot-detail-lookmore-icon.png' mode='aspectFit'></image>
